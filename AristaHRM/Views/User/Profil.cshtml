                            }).GetHtml()
                            @Html.DevExpress().DockZone(settings =>
                            {
                                settings.Name = "LHS";
                                settings.ZoneUID = "LHS";
                                settings.ControlStyle.CssClass = "leftZone";
                                settings.Width = 550;
                                settings.PanelSpacing = 3;
                            }).GetHtml()
                            </td>
                            <td style="text-align:center; vertical-align:top">
                            @Html.DevExpress().DockPanel(panel =>
                            {
                                panel.Name = "PhotoPanel";
                                panel.PanelUID = "PhotoImage";
                                panel.HeaderText = "Foto Karyawan";
                                panel.Width = Unit.Percentage(40);
                                panel.Height = Unit.Percentage(33);
                                panel.OwnerZoneUID = "RHS";
                                panel.VisibleIndex = 0;
                                panel.ShowOnPageLoad = true;
                                panel.AllowedDockState = AllowedDockState.All;
                                panel.AllowResize = true;
                                panel.ShowCloseButton = false;
                                panel.SetContent(() =>
                                {
                                    Html.DevExpress().Image(settings =>
                                    {
                                        settings.Name = "PhotoImage";
                                        settings.ImageUrl = Html.Encode(Session["ImagePath"].ToString().Trim());
                                        settings.Width = Unit.Pixel(150);
                                        settings.Height = Unit.Pixel(200);
                                        settings.Theme = AristaHRM.Theme.SelectTheme;
                                        settings.Properties.AlternateText = "Foto Karyawan";
                                        settings.Style.Add("border", "2px solid");
                                    }).GetHtml();
                                    ViewContext.Writer.Write(@"<p style=""font:12px Arial; text-align:center"">Ubah foto: <input type=""file"" id=""filerequest"" name=""filerequest"" style=""border: 1px solid"" /></p>");
                                    Html.DevExpress().Button(button =>
                                    {
                                        button.Name = "btnUpload";
                                        button.Text = "Ubah Foto Profil";
                                        button.UseSubmitBehavior = true;
                                        button.Images.Image.Url = "~/Images/Image.png";
                                        button.Images.Image.Width = Unit.Pixel(15);
                                        button.Images.Image.Height = Unit.Pixel(15);
                                        button.Theme = AristaHRM.Theme.SelectTheme;
                                        button.RouteValues = DevExpressHelper.GetUrl(new { Controller = "User", Action = "Profil", id = "photo" });
                                    }).GetHtml();
                                });
                            }).GetHtml()
                            @Html.DevExpress().DockPanel(panel =>
                            {
                                panel.Name = "GaugePanel";
                                panel.PanelUID = "Gauge";
                                panel.HeaderText = "Sisa Cuti Tahunan";
                                panel.Width = Unit.Percentage(40);
                                panel.Height = Unit.Percentage(33);
                                panel.OwnerZoneUID = "RHS";
                                panel.VisibleIndex = 1;
                                panel.ShowOnPageLoad = true;
                                panel.AllowedDockState = AllowedDockState.All;
                                panel.AllowDragging = false;
                                panel.AllowResize = true;
                                panel.ShowCloseButton = false;
                                panel.SetContent(() =>
                                {
                                    Html.DevExpress().FormLayout(settings =>
                                    {
                                        settings.Name = "GaugeLayout";
                                        settings.AlignItemCaptionsInAllGroups = true;
                                        settings.SettingsItemCaptions.HorizontalAlign = FormLayoutHorizontalAlign.Right;
