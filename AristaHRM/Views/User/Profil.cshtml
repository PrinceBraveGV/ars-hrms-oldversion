                                            layoutitem.Caption = string.Empty;
                                            layoutitem.ShowCaption = DefaultBoolean.False;
                                            layoutitem.SetNestedContent(() =>
                                            {
                                                ViewContext.Writer.Write("<table width=\"100%\" style=\"text-align:center\">");
                                                ViewContext.Writer.Write("<tr>");
                                                ViewContext.Writer.Write("<td style=\"text-align:center; border:3px 3px 3px 3px solid; width:88%\">");
                                                ViewContext.Writer.Write("<div id=\"gaugebar\" style=\"height:12px; float:left; border:3px 3px 3px 3px solid\"></div><div id=\"emptybar\" style=\"height:12px; float:right; \"></div>");
                                                ViewContext.Writer.Write("</td>");
                                                ViewContext.Writer.Write("<td style=\"width:3%\">&nbsp;</td>");
                                                ViewContext.Writer.Write("<td style=\"width:3%\"><label id=\"SC\" style=\"text-align:center; font:12px arial\">" + ViewData["SisaCuti"] + "</label></td>");
                                                ViewContext.Writer.Write("<td style=\"width:3%\"><label id=\"SL\" style=\"text-align:center; font:12px arial\">/</label></td>");
                                                ViewContext.Writer.Write("<td style=\"width:3%\"><label id=\"TC\" style=\"text-align:center; font:12px arial\">" + ViewData["TotalCuti"] + "</label></td>");
                                                ViewContext.Writer.Write("</tr>");
                                                ViewContext.Writer.Write("</table>");
                                                ViewContext.Writer.Write("<br />");
                                            });
                                        });
                                    }).GetHtml();
                                });
                            }).GetHtml()
                            <input id="SisaCuti" value="@ViewData["sisacuti"]" type="hidden" />
                            <input id="TotalCuti" value="@ViewData["totalcuti"]" type="hidden" />

                            @Html.DevExpress().DockPanel(panel =>
                            {
                                panel.Name = "NotesPanel";
                                panel.PanelUID = "Notes";
                                panel.HeaderText = "Catatan dari Atasan/Supervisor";
