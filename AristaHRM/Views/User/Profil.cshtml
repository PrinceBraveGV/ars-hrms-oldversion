                                        {
                                            layoutitem.Caption = "Provinsi ";
                                            layoutitem.SetNestedContent(() =>
                                            {
                                                Html.DevExpress().TextBoxFor(m => m.Provinsi, textbox =>
                                                {
                                                    textbox.Text = Html.Encode(ViewBag.Alamat);
                                                    textbox.Width = Unit.Pixel(300);
                                                    textbox.Theme = AristaHRM.Theme.SelectTheme;
                                                    textbox.ReadOnly = true;
                                                    textbox.Style.Add("border", "none");
                                                    textbox.Style.Add("background-color", "white");
                                                }).GetHtml();
                                            });
                                        });
                                        settings.Items.Add(layoutitem =>
                                        {
                                            layoutitem.Caption = "Kota ";
                                            layoutitem.SetNestedContent(() =>
                                            {
                                                Html.DevExpress().TextBoxFor(m => m.Kota, textbox =>
                                                {
                                                    textbox.Text = Html.Encode(ViewBag.Alamat);
                                                    textbox.Width = Unit.Pixel(300);
                                                    textbox.Theme = AristaHRM.Theme.SelectTheme;
                                                    textbox.ReadOnly = true;
                                                    textbox.Style.Add("border", "none");
                                                    textbox.Style.Add("background-color", "white");
                                                }).GetHtml();
                                            });
                                        });
                                        settings.Items.Add(layoutitem =>
                                        {
                                            layoutitem.Caption = "Alamat ";
                                            layoutitem.SetNestedContent(() =>
                                            {
                                                Html.DevExpress().MemoFor(m => m.Alamat, textarea =>
                                                {
                                                    textarea.Properties.Columns = 50;
                                                    textarea.Properties.Rows = 3;
                                                    textarea.Theme = AristaHRM.Theme.SelectTheme;
                                                    textarea.ReadOnly = true;
                                                    textarea.Style.Add("border", "none");
                                                    textarea.Style.Add("background-color", "white");
                                                }).GetHtml();
                                            });
                                        });
                                        settings.Items.Add(layoutitem =>
                                        {
                                            layoutitem.Caption = "Agama ";
                                            layoutitem.SetNestedContent(() =>
                                            {
                                                Html.DevExpress().TextBoxFor(m => m.Agama, textbox =>
                                                {
                                                    textbox.Text = Html.Encode(ViewBag.Agama);
                                                    textbox.Width = Unit.Pixel(200);
                                                    textbox.Theme = AristaHRM.Theme.SelectTheme;
                                                    textbox.ReadOnly = true;
                                                    textbox.Style.Add("border", "none");
                                                    textbox.Style.Add("background-color", "white");
                                                }).GetHtml();
                                            });
                                        });
                                        settings.Items.Add(layoutitem =>
                                        {
                                            layoutitem.Caption = "Status Pernikahan ";
                                            layoutitem.SetNestedContent(() =>
