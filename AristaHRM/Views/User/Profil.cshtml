                                panel.Height = Unit.Percentage(34);
                                panel.OwnerZoneUID = "RHS";
                                panel.VisibleIndex = 1;
                                panel.ShowOnPageLoad = true;
                                panel.AllowedDockState = AllowedDockState.All;
                                panel.AllowDragging = false;
                                panel.AllowResize = true;
                                panel.ShowCloseButton = false;
                                panel.SetContent(() =>
                                {
                                    Html.DevExpress().MemoFor(m => m.Notes, settings =>
                                    {
                                        settings.Name = "Notes";
                                        settings.Properties.Width = Unit.Percentage(100);
                                        settings.Properties.Height = Unit.Percentage(80);
                                        settings.Properties.Columns = 53;
                                        settings.Properties.Rows = 5;
                                        settings.Properties.NullText = "Belum ada catatan yang dibuat saat ini.";
