@using AristaHRM.Reports

@Html.DevExpress().DocumentViewer(settings =>
{
       settings.Name = "ReportListKaryawanViewer";
       settings.Report = (ReportListKaryawan)ViewData["ReportListKaryawan"];
       settings.ToolbarMode = DocumentViewerToolbarMode.ExternalRibbon;
       settings.AssociatedRibbonName = "Ribbon";
       settings.StylesSplitter.SidePaneWidth = 290;
       settings.Theme = AristaHRM.Theme.SelectTheme;
       settings.CallbackRouteValues = new { Controller = "Report", Action = "ReportListKaryawanPartial" };
       settings.ExportRouteValues = new { Controller = "Report", Action = "EksporListKaryawan" };
}).GetHtml()
