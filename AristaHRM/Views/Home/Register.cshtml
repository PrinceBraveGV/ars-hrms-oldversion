                                                }).BindList(Providers.GetPerusahaan()).GetHtml();
                                            });
                                        });
                                        settings.Items.Add(layoutitem =>
                                        {
                                            layoutitem.Caption = "Cabang ";
                                            layoutitem.SetNestedContent(() =>
                                            {
                                                Html.RenderPartial("_Cabang", Model);
                                                // }).BindList(SQLRegister5).GetHtml();
                                            });
