                                                    dateedit.Properties.MaxDate = DateTime.Now.AddYears(-18);
                                                    dateedit.Properties.ValidationSettings.Display = Display.None;
                                                    dateedit.Properties.ValidationSettings.ErrorDisplayMode = ErrorDisplayMode.ImageWithText;
